{
  "name": "server",
  "private": true,
  "version": "0.3.5",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:prod": "node bin/www.js",
    "start:dev": "nodemon",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:restart": "docker-compose restart",
    "db:migrate": "ts-node -r tsconfig-paths/register bin/migrate",
    "db:seed": "ts-node -r tsconfig-paths/register bin/seed",
    "redis:migrate": "ts-node -r tsconfig-paths/register bin/redis-migrate",
    "redis:delete": "ts-node -r tsconfig-paths/register bin/redis-delete",
    "build": "tsc --sourceMap false",
    "test": "jest --coverage --verbose",
    "format": "prettier --write \"**/*.{spec.ts,ts,tsx,js,jsx,json,scss,css,md}\"",
    "lint": "tsc --noEmit && eslint . --cache --ext .ts,.tsx",
    "lint:fix": "tsc --noEmit && eslint . --cache --fix --ext .ts,.tsx",
    "lint:type": "tsc --noEmit",
    "precommit": "tsc --noEmit && lint-staged"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mariadb": "^2.1.2",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "query-string": "^6.9.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/http-status": "^1.1.2",
    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.11",
    "@types/passport": "^1.0.1",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/query-string": "^6.3.0",
    "@types/redis": "^2.8.14",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,scss,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json",
        "enableTsDiagnostics": true
      }
    },
    "moduleNameMapper": {
      "^app$": "<rootDir>/src/app",
      "^env$": "<rootDir>/src/env",
      "^models$": "<rootDir>/src/models",
      "^routes/(.*)$": "<rootDir>/src/routes/$1",
      "^services$": "<rootDir>/src/services",
      "^services/(.*)$": "<rootDir>/src/services/$1",
      "^types/(.*)$": "<rootDir>/src/types/$1",
      "^utils/(.*)$": "<rootDir>/src/utils/$1"
    }
  }
}
